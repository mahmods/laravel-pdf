{
    "name": "mahmods/laravel-pdf",
    "description": "Generate PDFs in Laravel with this mPDF wrapper.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "mpdf/mpdf": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "mahmods\\LaravelPdf\\": "src/LaravelPdf"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mahmods\\LaravelPdf\\PdfServiceProvider"
            ],
            "aliases": {
                "PDF": "mahmods\\LaravelPdf\\Facades\\Pdf"
            }
        }
    }
}
